{
  "name": "api-worker",
  "main": "src/index.ts",
  "compatibility_date": "2025-07-26",
  "dev": {
    "port": 8787
  },
  "containers": [
    {
      "class_name": "ExecutionSandbox",
      "image": "./sandbox.Dockerfile",
      "instance_type": "basic",
      "max_instances": 80
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "name": "RATE_LIMITER",
        "class_name": "RateLimiter"
      },
      {
        "name": "ExecutionSandbox",
        "class_name": "ExecutionSandbox"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["RateLimiter", "Sandbox"]
    },
    {
      "tag": "v2",
      "renamed_classes": [
        {
          "from": "Sandbox",
          "to": "ExecutionSandbox"
        }
      ]
    }
  ],
  "vars": {
    "ENVIRONMENT": "development"
  },
  "env": {
    "local": {
      "vars": {
        "ENVIRONMENT": "local"
      }
    }
  },
  "observability": {
    "enabled": true
  }
}
