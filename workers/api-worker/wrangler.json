{
  "name": "api-worker",
  "main": "src/index.ts",
  "compatibility_date": "2025-07-26",
  "dev": {
    "port": 8787
  },
  "containers": [
    {
      "class_name": "Sandbox",
      "image": "./sandbox.Dockerfile",
      "max_instances": 30
    }
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "ukoly-db",
      "database_id": "local"
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "name": "RATE_LIMITER",
        "class_name": "RateLimiter"
      },
      {
        "name": "Sandbox",
        "class_name": "Sandbox"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["RateLimiter", "Sandbox"]
    }
  ],
  "vars": {
    "SECRET_TOKEN": "dev-secret-token-123",
    "ENVIRONMENT": "development"
  },
  "env": {
    "local": {
      "vars": {
        "ENVIRONMENT": "local"
      }
    }
  }
}
